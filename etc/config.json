{
  "general": {
    "paths": {
      "base": "$(DYNAMO_BASE)",
      "archive": "$(DYNAMO_ARCHIVE)",
      "spool": "$(DYNAMO_SPOOL)",
      "datadir": "$(DYNAMO_DATADIR)"
    }
  },
  "history": {
    "persistency": {
      "module": "MySQLHistory",
      "config": {
        "db_params": {
          "config_file": "/etc/my.cnf",
          "config_group": "mysql-dynamo",
          "db": "dynamohistory"
        }
      }
    },
    "cache": {
      "db_params": {
        "config_file": "/etc/my.cnf",
        "config_group": "mysql-dynamo",
        "db": "dynamohistory_cache"
      }
    }
  },
  "registry": {
    "db_params": {
      "config_file": "/etc/my.cnf",
      "config_group": "mysql-dynamo",
      "db": "dynamoregister"
    }
  },
  "webservice": {
    "x509_key": "$(X509_USER_PROXY)",
    "cookie_file": "$(DYNAMO_SPOOL)/cookies.txt",
    "num_attempts": 20,
    "cache_db_params": {}
  },
  "phedex": {
    "url": "https://cmsweb.cern.ch/phedex/datasvc/json/prod"
  }
}

{
  "general": {
    "threads": 32,
    "paths": {
      "base": "$(DYNAMO_BASE)",
      "archive": "$(DYNAMO_ARCHIVE)",
      "spool": "$(DYNAMO_SPOOL)",
      "datadir": "$(DYNAMO_DATADIR)"
    }
  },
  "system_diagnostics": {
    "debug_thread": true,
    "show_time_profile": true
  },
  "webservice": {
    "x509_key": "$(X509_USER_PROXY)",
    "cookie_file": "$(DYNAMO_SPOOL)/cookies.txt",
    "num_attempts": 20,
    "cache_db_params": {}
  },
  "inventory": {
    "persistency": {
      "module": "MySQLInventoryStore",
      "config": {
        "db_params": {"db": "dynamo"}
      }
    },
    "included_sites": ["T2_KR_KISTI"],
    "excluded_sites": ["T1_UK_RAL_ECHO_Disk", "T2_CH_CERNBOX", "T2_MY_UPM_BIRUNI"]
  },
  "history": {
    "persistency": {
      "module": "MySQLHistory",
      "config": {
        "db_params": {"db": "dynamohistory"}
      }
    },
    "cache": {
      "db_params": {"db": "dynamohistory_cache"}
    }
  },
  "registry": {
    "db_params": {
      "db": "dynamoregister"
    }
  },
  "mysql": {
    "config_file": "/etc/my.cnf",
    "config_group": "mysql-dynamo",
    "max_query_len": 100000
  },
  "debug": {
    "time_profile": true,
    "repeat_failed_thread": true,
    "included_groups": "*",
    "included_datasets": "/*/RunIISummer16*/*"
  }
}

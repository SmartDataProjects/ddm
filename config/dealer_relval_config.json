{
  "dealer": {
    "partition_name": "RelVal",
    "group_name": "RelVal",
    "copy_op": {
      "module": "PhEDExCopyInterface",
      "config": {
        "phedex": {
          "url_base": "https://cmsweb.cern.ch/phedex/datasvc/json/prod",
          "dbs_url": "https://cmsweb.cern.ch/dbs/prod/global/DBSReader",
          "auth_handler": "HTTPSCertKeyHandler",
          "auth_handler_conf": {},
          "num_attempts": 5
        },
        "chunk_size": 50
      }
    },
    "history": {
      "module": "MySQLHistory",
      "config": {
        "db_params": {
          "config_file": "/etc/my.cnf.d/dynamo.cnf",
          "config_group": "mysql",
          "db": "dynamohistory"
        },
        "cache_db_params": {
          "config_file": "/etc/my.cnf.d/dynamo.cnf",
          "config_group": "mysql",
          "db": "dynamohistory_cache"
        },
        "snapshots_spool_dir": "$(DYNAMO_SPOOL)/detox_snapshots",
        "snapshots_archive_dir": "$(DYNAMO_ARCHIVE)/detox_snapshots"
      }
    },
    "plugins": {
      "enforcer": {
        "priority": 1,
        "module": "EnforcerHandler",
        "config": {
          "enforcer": {
            "max_dataset_size": 50.0,
            "policy": "$(DYNAMO_BASE)/policies/common/enforcer_rules_relval.json"
          }
        }
      }
    },
    "attrs": {},
    "target_sites": ["T1_*_Disk", "T2_*"],
    "target_site_occupancy": 0.9,
    "max_site_pending_fraction": 0.03,
    "max_total_cycle_volume": 20
  },
  "log_level": "info"
}

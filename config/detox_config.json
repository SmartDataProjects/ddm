{
  "detox": {
    "deletion_per_iteration": 0.01,
    "deletion_op": {
      "module": "PhEDExDeletionInterface",
      "config": {
        "auto_approval": true,
        "allow_tape_deletion": false,
        "tape_auto_approval": false,
        "chunk_size": 50
      }
    },
    "copy_op": {
      "module": "PhEDExCopyInterface",
      "config": {
        "chunk_size": 50
      }
    },
    "detox_history": {
      "history_db": "dynamohistory",
      "cache_db": "dynamohistory_cache",
      "snapshots_spool_dir": "$(DYNAMO_SPOOL)/detox_snapshots",
      "snapshots_archive_dir": "$(DYNAMO_ARCHIVE)/detox_snapshots"
    },
    "attrs": {
      "MySQLReplicaLock": {
        "users": [],
        "db_params": {
          "db": "dynamoregister"
        }
      },
      "CRABAccessHistory": {
        "store": {
          "db": "dynamo"
        }
      },
      "ProtectedSiteTagger": {
        "sites": []
      },
      "DatasetRelease": {},
      "EnforcedProtectionTagger": {
        "enforcer": {
          "policy": "$(DYNAMO_POLICIES)/common/enforcer_rules_physics.json"
        }
      },
      "BlockReplicaRelativeAge": {
      }
    }
  },
  "applock": {
    "user": "dynamo",
    "role": "dynamo",
    "app": "detox",
    "db_params": {
      "db": "dynamoregister"
    }
  },
  "log_level": "info"
}

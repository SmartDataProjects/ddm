{
  "crabaccess": {
    "max_back_query": 7,
    "popdb": {
      "config": {
        "url_base": "https://cmsweb.cern.ch/popdb",
        "auth_handler": "HTTPSCertKeyHandler",
        "auth_handler_conf": {}
      }
    },
    "store": {}
  },
  "globalqueue": {
    "htcondor": {
      "config": {
        "collector": "cmsgwms-collector-global.cern.ch:9620",
        "schedd_constraint": "CMSGWMS_Type =?= \"crabschedd\""
      }
    },
    "store": {}
  },
  "mysql": {
    "db_params": {
      "config_file": "/etc/my.cnf.d/dynamo.cnf",
      "config_group": "mysql",
      "db": "dynamo"
    }
  },
  "log_level": "info"
}
